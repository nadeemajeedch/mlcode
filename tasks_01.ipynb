   "source": [
    "# Prime number \n",
    "\n",
    "num=int(input(\"Enter a Number: \"))\n",
    "if(num==2 or num==3 or num==5 or num==7):\n",
    "    print(num,\"is Prime\")\n",
    "elif(num%2!=0 and num%3!=0 and num%5!=0 and num%7!=0):\n",
    "    print(num,\"is Prime\")\n",
    "else:\n",
    "    print(num,\"is Not Prime\")\n",
    "\n"
